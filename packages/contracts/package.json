{
  "name": "hardhat-boilerplate",
  "version": "0.0.1",
  "description": "Boiler plate hardhat project typescript",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "lint": "npx solhint contracts/*.sol && npx eslint .",
    "lint:md": "npx markdownlint --ignore node_modules **/*.md",
    "format": "prettier \"contracts/**/*.sol\" \"**/*.ts\" \"**/*.md\" \"**/*.yml\"",
    "test": "npx hardhat test",
    "test:watch": "npx hardhat watch test",
    "coverage": "yarn build && npx hardhat coverage --temp artifacts --network coverage",
    "deployDAO": "npx hardhat deployDemoDAO"
  },
  "dependencies": {
    "@gnosis.pm/safe-contracts": "^1.2.0",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^3.4.1",
    "@typechain/ethers-v5": "^2.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "chai": "^4.2.0",
    "compound-protocol": "https://github.com/compound-finance/compound-protocol.git#1243a5e7498f943a6061a4dbf3b71041ec7c6c70",
    "dotenv": "^8.2.0",
    "eslint-plugin-react": "^7.22.0",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.0.26",
    "hardhat": "^2.0.8",
    "hardhat-deploy": "^0.7.0-beta.44",
    "hardhat-typechain": "^0.3.3",
    "hardhat-watcher": "^2.1.1",
    "minimetoken": "https://github.com/Giveth/minime.git",
    "ts-generator": "^0.1.1",
    "typechain": "^3.0.0"
  }
}
